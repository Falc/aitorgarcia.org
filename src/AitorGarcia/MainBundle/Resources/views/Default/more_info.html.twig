{% extends "::layout.html.twig" %}

{% block title %}{{ 'more_info.title' | trans }} - Aitor Garc√≠a{% endblock %}

{% block stylesheets %}
{% stylesheets filter="cssrewrite,?yui_css" output='css/frontend-*.css'
    'css/main.css'
    'bundles/aitorgarciamain/css/more-info.css'
%}
    <link href="{{ asset_url }}" rel="stylesheet" type="text/css" />
{% endstylesheets %}
{% endblock %}

{% block subnavigation %}
<h2>{{ 'more_info.subnavigation' | trans }}</h2>
{{ parent() }}
{% endblock %}

{% block page %}
<div id="block-source-code">
    <h3>{{ 'more_info.heading.source_code' | trans | raw }}</h3>
    <p>{{ 'more_info.paragraph.developed_with' | trans | raw }}</p>
    <p>{{ 'more_info.paragraph.get_source' | trans | raw }}</p>
</div>
<div id="block-icons">
    <h3>{{ 'more_info.heading.icons' | trans }}</h3>
    <p>{{ 'more_info.paragraph.yusuke_icons' | trans | raw }}</p>
</div>
{% endblock %}
